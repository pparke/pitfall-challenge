@startuml
class Player {
 - speed : int
 + lives : int
 + score : int
 + time : int
 + swinging : bool
 + falling : bool
 void jump()
 void die()
 void reduceScore(int amt)
 void reduceLives()
 void takeDamage(int amt)
}

class Crocodile {
 + mouthOpen : bool
}

class Barrel {
 - speed : int
}

class Vine {
 - speed : int
 void OnCollisionEnter()
}

class Obstacle {
  void OnCollisionEnter()
  + damage : int
}
@enduml
